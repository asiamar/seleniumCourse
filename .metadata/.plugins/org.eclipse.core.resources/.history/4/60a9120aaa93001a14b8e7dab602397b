
public class cwiczenia {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

        int[] tablica = new int[10];
        //wype³nienie tablicy
        for (int i = 0; i < 10; i++) {
            tablica[i] = i + 1;
            
            System.out.println(tablica);
        }

		
		
		
	}

}
